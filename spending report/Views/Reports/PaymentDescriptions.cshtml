@model spending_report.ViewModels.PaymentType
@{
    ViewBag.Title = "PaymentTypes";
    Layout = "~/Views/Shared/SpendingMaster.cshtml";
}

@using (Html.BeginForm("AddPaymentDescription", "Reports"))
{
    <fieldset>
        Type name:
        @Html.EditorFor(i => i.TypeName)

        Description:
        @Html.EditorFor(i => i.Description)
        
        <input type="submit" value="Send" />
    </fieldset>
}
